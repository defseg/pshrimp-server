<!-- slightly messy port of the already slightly messy React logic -->
<div>
  <h4 class='language-segments'><%= name %> (<%= thing.size %>)</h4>
  <table class='inventory'><tbody>
    <% thing.contents.map(function (y) { %>
      <tr>
        <% y.map(function (x) { %>
          <td>
            <%= x.join(' ') %>
          </td>
        <% }) %>
      </tr>
    <% }) %>
  </tbody></table>
</div>